#!/bin/env python2
import sys
import json

import box_thrower


if __name__ == '__main__':
    if len(sys.argv) != 2:
        print("Usage: box_thrower ")
        exit(1)
    with open(sys.argv[1]) as f:
        conf = [line.rstrip() for line in f.readlines()]
    b = box_thrower.GenericBox(conf, [], [], "", "ubuntu-12.04", "base_template")
    b.up()
    b.setup()
